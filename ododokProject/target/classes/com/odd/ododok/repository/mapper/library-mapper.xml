<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="Library">
 	
 	<select id="libraryAll" resultType = "library">
 		select * from odd_library
 	</select>

	<select id="libraryView" resultType = "library">
		select * from odd_library where l_number = #{l_number}
	</select>
 </mapper>